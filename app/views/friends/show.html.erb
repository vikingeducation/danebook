<%= content_for :usernav, true %>
<section class="post post-small col-xs-12">
  <div class="row">
    <header class="post-header col-xs-12">
      <h2>Friends</h2>
    </header>
  </div>
  <div class="row">
    <section class="post-content col-xs-12">
      <div class="row">
        <% @friends.each do |friend| %>
         <%= render partial: 'shared/friends/friend', locals: {user: friend} %>
        <% end %>
      </div>
    </section>
  </div>

</section>
