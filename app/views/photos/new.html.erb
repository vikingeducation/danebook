<div class="container">
  <div class="panel panel-default">
    <div class="panel-heading text-center">
      <h2>Photos</h2>
    </div>
    <div class="panel-body">
      <%=  form_for @photo do |photo_fields| %>
        <%= photo_fields.file_field :user_photo %>
        <%= photo_fields.submit "Upload Photo", class: "btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>