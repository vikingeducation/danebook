var photo = $('[data-id='+<%= @photo.id %>+'].photo')
photo.find('.like_link').html('Like');
photo.find('.like_link').attr('href', '/photos/'+<%= @photo.id %>+'/like');
var num = Math.max(Number(photo.find('.x-people-like-this').html().split(" ")[0]) - 1, 0);
photo.find('.x-people-like-this').html(''+num+' people like this')
