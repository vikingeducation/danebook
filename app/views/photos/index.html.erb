            <div class="panel-heading row">
                <div class="panel-title">
                    <h4 class="col-xs-offset-5 col-xs-2 text-center">
                        Photos
                    </h4>

                  <% if current_user.id == @user.id %>
                    <%= button_to "Add Photo!", new_user_photo_path(@user), :method => "get", class: "btn btn-info col-xs-2 col-xs-offset-3" %>
                  <% end %>

                </div>
            </div>
            <div class="panel-body">
              <div class="row">

                <% @photos.each do |photo| %>
                  <%= render partial: 'picture_grid', locals: { user: @user, photo: photo } %>
