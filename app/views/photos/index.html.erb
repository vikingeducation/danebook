<%= render '/users/nav' %>
<%= render '/users/header'%>



          <div id="info-wrapper" class="container">
            <div id="about-row" class="row">
              <div class="col-xs-4 col-xs-offset-4">
                <h2 id="about-col"><strong>Photos</strong></h2>
              </div>
              <div  class="col-xs-4">
                  <%= link_to "Add Photo", new_user_photo_path(@user), :method => :get, class: "btn btn-primary", id: "edit_btn" %>


                </div>

            </div>

            <div id="about-content" class="friends-content row">
                <% @photos.each do |photo|%>
                  <div class="photo-thumb col-xs-3">
                    <% if @user.friends.include?(@current_user) || @user == @current_user %>
                    <a href="<%=user_photo_path(@user, photo)%>" title="<%=photo.created_at%>">
                      <img class="img-responsive" src="<%=photo.data.url(:thumb)%>">
                    </a>
                    <%else %>
                    <img class="img-responsive" src="<%=photo.data.url(:thumb)%>">
                    <%end%>
                  </div>
                  <%end%>

            </div>

            </div>
          </div>
