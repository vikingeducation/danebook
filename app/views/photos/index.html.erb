
  <main>

  <!-- Profile Header -->

  <%= render partial: "shared/menu", :locals => { :active => 'Timeline', :profile => @profile } %>

  <!-- Photos Header -->

  <div class="container">
    <div class="row test">
      <div class="col-xs-2 col-xs-offset-5">
        <h3>Photos</h3>
      </div>
      <div class="col-xs-5 edit-btn">
      <% if current_user.id == @profile.user_id %>
      <%= link_to "Add Photo", new_user_photo_path(@profile.user_id), class: "btn btn-primary pull-right" %>
      <% end %>
      </div>
    </div>

    <div class="row main-content">

          <% @photos.each do |photo| %>
      <div class="col-sm-3">
        <div class="photo-container">
            <%= link_to user_photo_path(@profile.user_id, photo) do%>
              <%= image_tag photo.photo.expiring_url(10)%>

              <div class="photo-overlay">
                <strong><%= photo.created_at%></strong>
              </div>
            <% end %>
        </div>
      </div>
          <% end %>
      
    </div>

  </main>