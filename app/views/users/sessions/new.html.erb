<%= simple_form_for(resource, as: resource_name, url: new_user_session_path, html: { class: 'navbar-form navbar-right'}) do |f| %>
  <%= f.input :email, required: false, autofocus: true %>
  <%= f.input :password, required: false %>
  <%= f.button :submit, "Sign in", class: 'btn btn-action' %>
<% end %>
