<section class="row" id="timeline-about">
  <header class="col-md-12">
    <h4>About</h4>
  </header>

  <div class="row">
    <div class = "col-xs-5">
      <b>Born on:</b>
    </div>
    <div class = "col-xs-7">
      <%= user.birthdate %>
    </div>
  </div>

  <div class="row">
    <div class = "col-xs-5">
      <b>Went to school at:</b>
    </div>
    <div class = "col-xs-7">
      <%= profile.college %>
    </div>
  </div>

  <div class="row">
    <div class = "col-xs-5">
      <b>Hometown:</b>
    </div>
    <div class = "col-xs-7">
      <%= profile.hometown %>
    </div>
  </div>

  <div class="row">
    <div class = "col-xs-5">
      <b>Currently Lives:</b>
    </div>
    <div class = "col-xs-7">
      <%= profile.location %>
    </div>
  </div>
</section>