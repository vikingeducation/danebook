<section id="interior-page">

  <%= render partial: "shared/header", locals: { user: @user } %>

  <main class="container">
    <div class="row">
      <div class="panel panel-default col-xs-offset-1 col-xs-10">

      
        <div class="row bottom-box">
          <div class="col-md-6 col-md-push-6 col-lg-7 col-lg-push-5">
            <%= render partial: "shared/post_form", locals: { user: @user, post: @post } %>
            <%= render partial: "shared/posts", locals: { user: @user, post: @post, posts: @posts } %>
          </div>
          <div class="col-md-6 col-md-pull-6 col-lg-5 col-lg-pull-7">
            <%= render partial: "shared/about", locals: { user: @user } %>
            <%= render partial: "shared/photos", locals: { user: @user } %>
            <%= render partial: "shared/friends", locals: { user: @user } %>
          </div>
        </div>

      </div>
    </div>
  </main>

</section>

