<div class="row post-control">
  <div class="col-xs-12">
    <div class="row">
      <div class="col-xs-12 selected post-topbar">
        <div class="center"> Photos (123) (Copied Text Area)</div>
      </div>
    </div>

  <!-- We can just put them all in one row since cols that go past 12 overflow into a new row! -->

    <div class="row timeline-photos">
      <% @profile.user.photos.each do |photo| %>
        <div class="col-xs-4">
          <%= link_to image_tag( photo.photo_data.url(:thumb) ), user_photo_path(@profile.user, photo) %>
        </div>
      <% end %>

      <div class="row">
        <div class="col-xs-12">
          <div class="center"><a href="photos.html">See More Photos</a></div>
        </div>
      </div>

    </div>
  </div>
</div>
  