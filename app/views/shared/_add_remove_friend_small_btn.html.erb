<% if user_id.to_s != current_user.id.to_s %>
  <% if current_user.friended_users.pluck(:id).include?(user_id) %>
    <%= link_to "Remove Friend", friending_path(:user_id => user_id), method: 'delete', class: "btn btn-default btn-sm unfriend pull-right" %>
  <% else %>
    <%= link_to "Friend me", friending_path(:user_id => user_id), method: 'post', class: "btn btn-default btn-sm unfriend pull-right" %>
  <% end %>
<% end %>
